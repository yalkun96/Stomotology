#: myenv/lib/python3.11/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "Rozšíření pro PostgreSQL"

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/fields/array.py:21
#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/array.py:17
#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Položka č. %(nth)s v seznamu je neplatná:"

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Vnořená pole musejí mít stejnou délku."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Mapování řetězců na řetězce či hodnoty NULL"

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Hodnota s klíčem \"%(key)s\" není řetězec ani NULL."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Data typu JSON nelze načíst."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Vstup musí být slovník formátu JSON."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Zadejte dvě platné hodnoty."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Počáteční hodnota rozsahu nemůže být vyšší než koncová hodnota."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Zadejte dvě celá čísla."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Zadejte dvě čísla."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Zadejte dvě platné hodnoty data nebo času."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Zadejte dvě platná data."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Seznam obsahuje %(show_value)d položku, ale neměl by obsahovat více než "
"%(limit_value)d."
msgstr[1] ""
"Seznam obsahuje %(show_value)d položky, ale neměl by obsahovat více než "
"%(limit_value)d."
msgstr[2] ""
"Seznam obsahuje %(show_value)d položek, ale neměl by obsahovat více než "
"%(limit_value)d."
msgstr[3] ""
"Seznam obsahuje %(show_value)d položek, ale neměl by obsahovat více než "
"%(limit_value)d."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Seznam obsahuje %(show_value)d položku, ale neměl by obsahovat méně než "
"%(limit_value)d."
msgstr[1] ""
"Seznam obsahuje %(show_value)d položky, ale neměl by obsahovat méně než "
"%(limit_value)d."
msgstr[2] ""
"Seznam obsahuje %(show_value)d položek, ale neměl by obsahovat méně než "
"%(limit_value)d."
msgstr[3] ""
"Seznam obsahuje %(show_value)d položek, ale neměl by obsahovat méně než "
"%(limit_value)d."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Některé klíče chybí: %(keys)s"

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Byly zadány neznámé klíče: %(keys)s"

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:81
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely greater than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr "Nejnižší hodnota rozsahu musí být větší nebo rovna %(limit_value)s."

#: myenv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:90
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely less than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr "Nejvyšší hodnota rozsahu musí být menší nebo rovna %(limit_value)s."
